{
  "build": {
    "content": [
      {
        "files": [
		  "PowerShell/**/**.md",
		  "PowerShell/**/toc.yml",
		  "PowerShell/**.md",
		  "PowerShell/toc.yml",
		  "toc.yml",
		  "*.md"
		]
      },
	  {
		"files": [
		  "Console Apps/**/**.md",
		  "Console Apps/**/toc.yml",
		  "Console Apps/**.md",
		  "Console Apps/toc.yml",
		  "toc.yml",
		  "*.md"
		]
	  },
	  {
		"files": "General Documentation/**.md",
	  },
	  {
		"files": "*.{md,yml}"
	  }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
	"markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false,
	"globalMetadata": {
	  "_appTitle": "DW Scripts Lib",
	  "_gitContribute": {
	    "repo": "https://github.com/dlw-digitalworkplace/dw-script-lib",
		"branch": "main"
	  },
	  "_description": "This library will give you explanations on handy scripts created by your colleagues.",
	  "_enableNewTab": true,
	  "_enableSearch": true
	}
  }
}